{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2,"never"],
    "no-cond-assign": [2,"always"],
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-extra-semi": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "valid-typeof": 2,
    "curly": [2,"all"],
    "dot-notation": 2,
    "no-alert": 2,
    "no-else-return": 2,
    "no-extra-bind": 1,
    "no-fallthrough": 2,
    "no-multi-spaces": 2,
    "no-redeclare": 2,
    "no-script-url": 2,
    "yoda": [1,"never",{}],
    "array-bracket-spacing": [2,"never",{}],
    "brace-style": [2,"stroustrup",{"allowSingleLine":true}],
    "comma-spacing": [2,{"after":true}],
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2,{"max":1}],
    "arrow-spacing": [2,{"before":true,"after":true}],
    "constructor-super": 2,
    "no-const-assign": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2
  },
  "plugins": [
    "babel",
    "react"
  ]
}